<div class='events'>
	<h3>Event:</h3>
	<hr />
	<h2><%= @event.name%></h2>

	<%= image_tag @event.image, size: "100"%><br>

	<p><strong>Type: </strong><%= @event.category %></p>
	<p><strong>Coords: </strong><%= @event.coords %></p>
	<p>
	  <strong>Level Range:</strong>
	  <%= @event.level_min %> to
	  <%= @event.level_max %>
	</p>

	<%= link_to "Edit", edit_event_path %>
	<%= link_to "Back", events_path %>
</div>