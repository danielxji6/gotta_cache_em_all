<p id="notice"><%= notice %></p>

<h3>Events</h3><hr>

<% @events.each do |event| %>
  <p>Pokemon name: <%= link_to event.name, event_path(event) %></p>
  <p>Type: <%= event.category %></p>
  <p>Coords: <%= event.coords %></p>
  <hr>
<% end %>


<iframe width="600" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJIQBpAG2ahYAR_6128GcTUEo&key=<%= ENV["GOOGLE_MAPS"] %>" ></iframe>

