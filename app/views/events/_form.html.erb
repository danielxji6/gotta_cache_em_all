<%= form_for(@event) do |f| %>
  <fieldset>
  <% if @event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% @event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :name, "Pokemon Name", class: "col-md-4 control-label" %><br>
    <div class="col-md-8">
      <%= f.text_field :name %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :coords, class: "col-md-4 control-label" %><br>
    <div class="col-md-8">
      <%= f.text_field :coords %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :catagory, "Type", class: "col-md-4 control-label" %><br>
    <div class="col-md-8">
      <%= f.text_field :category %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :image, "Image URL", class: "col-md-4 control-label" %><br>
    <div class="col-md-8">
      <%= f.text_field :image %>
    </div>
  </div>
  <div class="actions">
    <div class="col-md-12">
      <%= f.submit class: "btn btn-success" %>
    </div>
  </div>
  </fieldset>
<% end %>
